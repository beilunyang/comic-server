{
  "name": "api-server",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "clean": "rimraf dist",
    "build": "NODE_ENV=production babel src --out-dir dist",
    "debug": "babel src --out-dir debug --source-maps",
    "serve": "node dist/main",
    "start:babel": "babel-node src/main",
    "start": "NODE_ENV=develop nodemon --watch src --exec npm run start:babel -L",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha --compilers js:babel-core/register test/**/*.js --bail -t 20000"
  },
  "author": "beilunyang",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.4",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-preset-env": "^1.1.8",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.3.0",
    "install": "^0.8.1",
    "nodemon": "^1.11.0",
    "npm": "^3.10.8",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "ioredis": "^3.0.0",
    "kcors": "^2.2.0",
    "koa": "^2.2.0",
    "koa-bodyparser": "^3.2.0",
    "koa-router": "^7.0.1",
    "log4js": "^1.1.1",
    "mongoose": "^4.7.7",
    "swagger-jsdoc": "^1.8.2"
  },
  "license": "MIT"
}
